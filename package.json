{
  "name": "mviwo",
  "description": "Health Records Management in a Challenging Environment",
  "version": "0.2.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run build & concurrently --kill-others \"npm run serve-api\" \"npm run serve\"",
    "serve": "ng serve --aot --proxy-config proxy.conf.json",
    "build": "ng build --prod --output-path=public --stats-json",
    "serve-api": "nodemon server",
    "test": "ng test",
    "test-server": "node_modules/.bin/mocha server/test/metric --recursive",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "push": "git push & git push heroku master",
    "sw": "sw-precache --root=public --config=sw-precache-config.js",
    "bundle-report": "webpack-bundle-analyzer public/stats.json",
    "start-mongo": "md mongo-db & \"C:/Program Files/MongoDB/Server/3.6/bin/mongod.exe\" --dbpath mongo-db"
  },
  "private": true,
  "dependencies": {
